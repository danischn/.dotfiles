#!/usr/bin/env bash

#Url is the most recently copied element in the clipboard
url=$(pbpaste)

# Check if the download was successful
if  wget -q -P ~/Documents/skole/3.sem/pdfs/ "$url" ; then
  filename=$(basename "$url")
  pdf_path=~/Documents/skole/3.sem/pdfs/"$filename"
  open "$pdf_path"
  exit 0
else
  echo "Failed to download the url" 
  exit 1
fi
